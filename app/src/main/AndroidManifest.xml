<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="softeng2.teamhortons.myxa">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/myxa_logo_bg"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/myxa_logo_bg"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".RiderSignUpActivity"/>
        <activity android:name=".GeneralRegistrationActivity" />
        <activity android:name=".CustomerRegistrationActivity" />
        <activity android:name=".ui.login.LoginActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>